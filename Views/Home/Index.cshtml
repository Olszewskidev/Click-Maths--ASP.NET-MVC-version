
@{
    ViewBag.Title = "Index";
}
<head>
    <link href="~/Content/jquery.tabSlideOut.css" rel="stylesheet" />
    <link href="~/Content/ionicons.css" rel="stylesheet" />
</head>
<div class="tab-content">
    <script src="~/Scripts/jquery-3.3.1.min.js"></script>
    <script src="~/Scripts/jquery.tabSlideOut.js"></script>
    <div id="UserPanel" style="width:20%; height:auto; background-color: lightsteelblue"><span class="handle">GamePanel</span>
        <div style="text-align: center;margin:auto">

            <label for="Gender"> Gender:</label>
            <div id="Gender">
                <img id="GenderImg" style="display:none"/>
                <div id="inputGender">
                    Male: <input type="checkbox" id="male" name="Gen" />
                    Female: <input type="checkbox" id="female" name="Gen" />
                </div>
            </div>
            <img src="~/AppFile/Img/geek.png" style="display:none"/>
            <br />
            <label for="ScoreText">Score:</label>
            <b id="ScoreText">0</b>
            <br />
            <label for="answer">Your Answer:</label>
            <input type="number" id="answer" />
            Remember me again:<input type="checkbox" name="cookieyes" />
        </div>
    </div>
    <div class="tab-pane fade in active" id="QuizContent">@Html.Action("Quizs10")</div>
    <div class="tab-pane fade in" id="AddContent">Tutaj dodawanie zagadek</div>
    <script src="~/Scripts/JSScript1.js"></script>

</div>